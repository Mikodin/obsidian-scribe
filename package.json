	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.15.3",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.3",
		"builtin-modules": "^5.0.0",
		"dotenv": "^16.4.7",
		"esbuild": "^0.25.0",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@ffmpeg/ffmpeg": "^0.12.15",
		"@ffmpeg/util": "^0.12.2",
		"@fix-webm-duration/fix": "^1.0.1",
		"@langchain/openai": "^1.1.3",
		"assemblyai": "^4.9.0",
		"langchain": "^1.1.2",
		"mini-debounce": "^1.0.8",
